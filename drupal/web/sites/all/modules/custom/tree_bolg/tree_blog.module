/**
 * @file
 * A block module display recent blog posts about trees.
 */

/**
 * Implements hook_help().
 */
function tree_blog_help($path, $arg) {
  switch ($path) {
    case "admin/help#tree_blog":
      return t('Display link to nodes created on this date');
      break;
  }
}
